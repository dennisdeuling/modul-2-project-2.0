{{> success_msg}}
<div class="container-fluid">
	{{#unless apartments}}
		<div class="alert alert-dismissible alert-danger" role="alert">
			<p class="mb-0"><strong>I'm sorry, but for this query we don't have any results. Please try a new
				search.</strong></p>
		</div>
	{{/unless}}
	{{#each apartments}}
		{{>apartmentCard this}}
	{{/each}}
</div>